    "components": {
        "schemas": {
            "ErrorResponse": {
                "properties": {
                    "error": {
                        "type": "string",
                        "title": "Error",
                        "description": "Error message describing the issue.",
                        "example": "An error occurred"
                    }
                },
                "type": "object",
                "required": [
                    "error"
                ],
                "title": "ErrorResponse"
            },
            "FingridDataPoint": {
                "properties": {
                    "startTime": {
                        "type": "string",
                        "title": "Starttime",
                        "description": "Start time in RFC 3339 format (e.g., 2024-05-01T00:00:00Z)",
                        "example": "2024-05-01T00:00:00Z"
                    },
                    "endTime": {
                        "type": "string",
                        "title": "Endtime",
                        "description": "End time in RFC 3339 format (e.g., 2024-05-02T00:00:00Z)",
                        "example": "2024-05-02T00:00:00Z"
                    },
                    "value": {
                        "type": "number",
                        "title": "Value",
                        "description": "Value of the data point",
                        "example": "7883.61"
                    }
                },
                "type": "object",
                "required": [
                    "startTime",
                    "endTime",
                    "value"
                ],
                "title": "FingridDataPoint"
            },
            "HTTPValidationError": {
                "properties": {
                    "detail": {
                        "items": {
                            "$ref": "#/components/schemas/ValidationError"
                        },
                        "type": "array",
                        "title": "Detail"
                    }
                },
                "type": "object",
                "title": "HTTPValidationError"
            },
            "PriceDataPoint": {
                "properties": {
                    "startDate": {
                        "type": "string",
                        "title": "Startdate",
                        "description": "UTC str in RFC 3339 format",
                        "example": "2025-05-08T04:00:00.000Z"
                    },
                    "price": {
                        "type": "number",
                        "title": "Price",
                        "description": "Floating-point number representing the price in euro cents",
                        "example": 0.61
                    }
                },
                "type": "object",
                "required": [
                    "startDate",
                    "price"
                ],
                "title": "PriceDataPoint"
            },
            "TimeRangeRequest": {
                "properties": {
                    "startTime": {
                        "type": "string",
                        "title": "Starttime",
                        "description": "Start time in RFC 3339 format (e.g., 2024-05-01T00:00:00Z)",
                        "example": "2024-05-01T00:00:00Z"
                    },
                    "endTime": {
                        "type": "string",
                        "title": "Endtime",
                        "description": "End time in RFC 3339 format (e.g., 2024-05-02T00:00:00Z)",
                        "example": "2024-05-02T00:00:00Z"
                    }
                },
                "type": "object",
                "required": [
                    "startTime",
                    "endTime"
                ],
                "title": "TimeRangeRequest"
            },
            "User": {
                "properties": {
                    "email": {
                        "type": "string",
                        "format": "email",
                        "title": "Email"
                    },
                    "password": {
                        "type": "string",
                        "title": "Password"
                    },
                    "role": {
                        "anyOf": [
                            {
                                "type": "string"
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "title": "Role",
                        "default": "user"
                    }
                },
                "type": "object",
                "required": [
                    "email",
                    "password"
                ],
                "title": "User"
            },
            "ValidationError": {
                "properties": {
                    "loc": {
                        "items": {
                            "anyOf": [
                                {
                                    "type": "string"
                                },
                                {
                                    "type": "integer"
                                }
                            ]
                        },
                        "type": "array",
                        "title": "Location"
                    },
                    "msg": {
                        "type": "string",
                        "title": "Message"
                    },
                    "type": {
                        "type": "string",
                        "title": "Error Type"
                    }
                },
                "type": "object",
                "required": [
                    "loc",
                    "msg",
                    "type"
                ],
                "title": "ValidationError"
            },
            "WeatherDataPoint": {
                "properties": {
                    "temperature_celsius": {
                        "type": "number",
                        "title": "Temperature Celsius",
                        "description": "Air temperature in degrees Celsius based on the forecast.",
                        "example": 8.5
                    },
                    "wind_speed_mps": {
                        "type": "number",
                        "title": "Wind Speed Mps",
                        "description": "Wind speed in meters per second based on the forecast.",
                        "example": 2.2
                    },
                    "closest_forecast_time": {
                        "type": "string",
                        "title": "Closest Forecast Time",
                        "description": "Timestamp (in UTC, RFC 3339 format) of the closest available weather forecast.",
                        "example": "2025-05-09T16:00:00Z"
                    }
                },
                "type": "object",
                "required": [
                    "temperature_celsius",
                    "wind_speed_mps",
                    "closest_forecast_time"
                ],
                "title": "WeatherDataPoint"
            },
            "WeatherRequest": {
                "properties": {
                    "lat": {
                        "type": "number",
                        "title": "Lat",
                        "description": "Latitude",
                        "example": 60.1699
                    },
                    "lon": {
                        "type": "number",
                        "title": "Lon",
                        "description": "Longitude",
                        "example": 24.9384
                    },
                    "timestamp": {
                        "type": "string",
                        "title": "Timestamp",
                        "description": "UTC str in RFC 3339 format, e.g. 2024-05-05T13:30:00Z",
                        "example": "2024-05-05T13:30:00Z"
                    }
                },
                "type": "object",
                "required": [
                    "lat",
                    "lon",
                    "timestamp"
                ],
                "title": "WeatherRequest"
            }
        }
    }
}